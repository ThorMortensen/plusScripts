#!/bin/bash


if [ -z "$1" ] 
then
  echo "Please enter IP address"
  exit 
fi 

echo "IP is $1"

auth-wrapper ssh -v support@$1 "mkdir -p ~/.ssh && touch ~/.ssh/authorized_keys && echo \"ssh-rsa  tm@connectedcars.dk\" >> ~/.ssh/authorized_keys"
# auth-wrapper ssh -v support@$1 "mkdir -p ~/.ssh && touch ~/.ssh/authorized_keys"
auth-wrapper ssh -v support@192.168.7.1 "mkdir -p ~/.ssh && touch ~/.ssh/authorized_keys && echo \"ssh-rsa  tm@connectedcars.dk\" >> ~/.ssh/authorized_keys"
auth-wrapper ssh -v support@192.168.7.1 "mkdir -p ~/.ssh && touch ~/.ssh/authorized_keys"
